
<?xml version="0.0" ?>
<robot name="box.urdf">
  <link name="baseLink">

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="0"/> <!--disable gravity-->
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
         <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>
</robot>
